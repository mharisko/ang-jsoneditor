{
  "name": "@maaxgr/ang-jsoneditor",
  "version": "12.0.0",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/MaaxGr/ang-jsoneditor"
  },
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:app": "npm run build:libcopy && ng build --project packages && rimraf src/ang-jsoneditor",
    "build:lib": "rimraf dist && ng-packagr -p ang-jsoneditor/package.json",
    "build:libcopy": "cp -R dist/ang-jsoneditor src/ang-jsoneditor",
    "watch": "onchange 'ang-jsoneditor/**/*.ts' -- npm run build:lib && npm run build:libcopy",
    "build:full": "npm run build:lib && npm run build:app",
    "publishnpm": "cp README.md ang-jsoneditor/README.md && npm run build:lib && cd dist/ang-jsoneditor && npm publish",
    "unpublish": "npm deprecate -f 'ang-jsoneditor@1.9.3' 'this package has been deprecated'",
    "test": "npm run test:dev && npm run e2e",
    "test:dev": "ng test dev --watch=false",
    "test:unit": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "ports": "lsof -i tcp:4201",
    "reinstall": "rm package-lock.json && rm -rf node_modules && npm i",
    "rebuildsass": "npm rebuild node-sass"
  },
  "dependencies": {
    "@angular/common": "^12.0.0",
    "@angular/compiler": "^12.0.0",
    "@angular/core": "^12.0.0",
    "@angular/forms": "^12.0.0",
    "@angular/platform-browser": "^12.0.0",
    "@angular/platform-browser-dynamic": "^12.0.0",
    "@angular/router": "^12.0.0",
    "core-js": "^3.6.4",
    "jsoneditor": "^9.5.7",
    "rxjs": "*",
    "webpack": "^5.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^12.0.0",
    "@angular-devkit/schematics": "^12.0.0",
    "@angular/cli": "^12.0.0",
    "@angular/compiler-cli": "^12.0.0",
    "@angular/language-service": "^12.0.0",
    "@types/jasmine": "3.3.8",
    "@types/node": "^13.7.1",
    "codelyzer": "~6.0.0",
    "cpr": "^3.0.0",
    "jasmine": "^3.3.0",
    "jasmine-core": "^3.3.0",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^2.0.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "ng-packagr": "^12.0.0",
    "protractor": "~7.0.0",
    "rimraf": "^2.7.1",
    "ts-node": "~7.0.0",
    "tsickle": "^0.43.0",
    "tslib": "^2.3.0",
    "tslint": "~6.1.3",
    "typescript": "4.4"
  }
}
